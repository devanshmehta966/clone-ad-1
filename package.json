{
  "name": "nextjs-marketing-dashboard",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate && prisma generate --schema=prisma/customer-schema.prisma",
    "db:push": "prisma db push && prisma db push --schema=prisma/customer-schema.prisma",
    "db:migrate": "prisma migrate dev && prisma migrate dev --schema=prisma/customer-schema.prisma",
    "db:migrate:prod": "prisma migrate deploy && prisma migrate deploy --schema=prisma/customer-schema.prisma",
    "db:seed": "tsx prisma/seed.ts && tsx prisma/customer-seed.ts",
    "db:seed:prod": "NODE_ENV=production tsx prisma/seed.ts && NODE_ENV=production tsx prisma/customer-seed.ts",
    "db:studio": "prisma studio",
    "db:studio:customer": "prisma studio --schema=prisma/customer-schema.prisma",
    "db:start": "docker-compose up -d postgres customer-db",
    "db:stop": "docker-compose down",
    "db:reset": "prisma db push --force-reset && prisma db push --force-reset --schema=prisma/customer-schema.prisma && yarn db:generate && yarn db:seed",
    "migrate:test-app": "node scripts/test-app-functionality.js",
    "security:test": "node scripts/security-test.js",
    "security:audit": "yarn audit --audit-level moderate",
    "security:check": "yarn security:audit && yarn security:test",
    "perf:test": "node scripts/test-performance.js",
    "perf:test:prod": "BASE_URL=https://your-production-url.com node scripts/test-performance.js",
    "analyze": "ANALYZE=true yarn build",
    "analyze:server": "BUNDLE_ANALYZE=server yarn build",
    "analyze:browser": "BUNDLE_ANALYZE=browser yarn build",
    "perf:lighthouse": "lighthouse http://localhost:3000 --output json --output html --output-path ./lighthouse-report",
    "perf:bundle": "yarn analyze && open ./analyze/client.html",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPatterns=__tests__/unit",
    "test:integration": "jest --testPathPatterns=__tests__/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:visual": "playwright test --grep='Visual Regression'",
    "test:all": "yarn test:unit && yarn test:integration && yarn test:e2e",
    "test:ci": "jest --coverage --ci && yarn test:e2e",
    "docker:build": "docker build -t marketing-dashboard .",
    "docker:run": "docker run -p 3000:3000 marketing-dashboard",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:prod:build": "docker-compose -f docker-compose.prod.yml up -d --build",
    "docker:prod:down": "docker-compose -f docker-compose.prod.yml down",
    "docker:logs": "docker-compose -f docker-compose.prod.yml logs -f",
    "deploy:vercel": "vercel --prod",
    "deploy:check": "yarn type-check && yarn lint && yarn test:ci",
    "postinstall": "prisma generate && prisma generate --schema=prisma/customer-schema.prisma"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.16.2",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-context-menu": "^2.2.15",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-hover-card": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.15",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tanstack/react-query": "^5.83.0",
    "@types/argon2": "^0.15.4",
    "argon2": "^0.44.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.462.0",
    "next": "^15.1.3",
    "next-auth": "^4.24.10",
    "next-themes": "^0.3.0",
    "prisma": "^6.16.2",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.62.0",
    "react-resizable-panels": "^2.1.9",
    "recharts": "^2.15.4",
    "sonner": "^1.7.4",
    "stripe": "^18.5.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.9",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^15.1.3",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^3.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.16.5",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.1.3",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "msw": "^2.11.2",
    "node-fetch": "^3.3.2",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
